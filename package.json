{
   "name": "DAAPIService",
   "version": "0.1.0",
   "description": "Node.js on Docker",
   "author": "Ruhao Xia <1994ep@gmail.com>",
   "main": "server.js",
   "scripts": {
      "lint": "eslint . --ext .ts",
      "lint-and-fix": "eslint . --ext .ts --fix",
      "start": "ts-node src/index.ts",
      "build": "tsc -p .",
      "dev": "nodemon src/index.ts",
      "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ormconfig.json",
      "prepare": "husky install"
   },
   "dependencies": {
      "axios": "^0.21.1",
      "cls-rtracer": "^2.6.0",
      "dotenv": "^9.0.1",
      "express": "^4.16.1",
      "http-status-codes": "^2.1.4",
      "joi": "^17.4.0",
      "jsonwebtoken": "^8.5.1",
      "mysql": "^2.14.1",
      "pg": "^8.6.0",
      "qs": "^6.10.1",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.32",
      "winston": "^3.3.3"
   },
   "devDependencies": {
      "@types/express": "^4.17.11",
      "@types/jsonwebtoken": "^8.5.1",
      "@types/node": "^8.0.29",
      "@types/qs": "^6.9.6",
      "@typescript-eslint/eslint-plugin": "^4.22.0",
      "@typescript-eslint/parser": "^4.22.0",
      "eslint": "^7.25.0",
      "husky": "^6.0.0",
      "nodemon": "^2.0.7",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run lint-and-fix"
      }
   }
}
